# f7a7a176770443fd845bcbdc8a69ccc9
#: ../../demos.rst:43
msgid ""
"function DecorateURLs (\n"
"    const AText : string;\n"
"    AFlags : TDecorateURLsFlagSet = [durlAddr, durlPath]\n"
") : string;\n"
"\n"
"type\n"
"TDecorateURLsFlags = (\n"
"    durlProto, durlAddr, durlPort, durlPath, durlBMark, durlParam);\n"
"\n"
"TDecorateURLsFlagSet = set of TDecorateURLsFlags;\n"
"\n"
"function DecorateEMails (const AText : string) : string;"
msgstr ""
"function DecorateURLs (const AText: string; AFlags: TDecorateURLsFlagSet "
"= [durlAddr, durlPath]): string; escriba TDecorateURLsFlags = (durlProto,"
" durlAddr, durlPort, durlPath, durlBMark, durlParam); "
"TDecorateURLsFlagSet = conjunto de TDecorateURLsFlags; función "
"DecorateEMails (const AText: string): string;"

